# Contents of the file: /culinary_agent_backend/culinary_agent_backend/.env.example

# Example environment variables for the Culinary Agent project

# Secret key for the LangFuse client
LANGFUSE_SECRET_KEY=your_langfuse_secret_key_here

# Model ID for the LLM (must be available on HuggingFace)
# Options: Qwen/Qwen2.5-7B-Instruct, meta-llama/Meta-Llama-3.1-8B-Instruct, mistralai/Mistral-7B-Instruct-v0.3
MODEL_ID=Qwen/Qwen2.5-7B-Instruct

# Path to the FAISS index file
FAISS_INDEX_PATH=recipe_index.faiss

# Path to the recipes metadata file
RECIPES_METADATA_PATH=recipes_for_embeddings.jsonl

# Path to the full recipes file
FULL_RECIPES_PATH=full_format_recipes.json

# Other configuration variables can be added as needed